#ifndef INC_STM32_IWDG_H_
#define INC_STM32_IWDG_H_


typedef struct
{
	volatile uint32_t KR;
	volatile uint32_t PR;
	volatile uint32_t RLR;
	volatile uint32_t SR;
}iwdg_structure;


